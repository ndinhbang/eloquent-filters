{
    "name": "ndinhbang/eloquent-filters",
    "description": "A laravel package to provides an elegant way to filter your eloquent model via the request query string",
    "keywords": [
        "laravel",
        "eloquent",
        "eloquent-filters"
    ],
    "homepage": "https://github.com/ndinhbang/eloquent-filters",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nguyen Dinh Bang",
            "email": "ndinhbang111@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ndinhbang\\EloquentFilters\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ndinhbang\\EloquentFilters\\EloquentFiltersServiceProvider"
            ],
            "aliases": {
                "EloquentFilters": "Ndinhbang\\EloquentFilters\\EloquentFiltersFacade"
            }
        }
    }
}
